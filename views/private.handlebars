  <p>titile: {{this.title}} </p>
  <p>image: {{this.image}} </p>
  <p>sizeOfApartment: {{this.sizeOfApartment}} </p>
  <p>petsAllowed: {{this.petsAllowed}} </p>
  <p>coupleAllowed: {{this.coupleAllowed}} </p>
<hr />
{{/each}}