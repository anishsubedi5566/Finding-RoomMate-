<div class="grid-container">
    <div class="grid-item">
      <h3>Message</h3>
<ul class="message_list">
  {{#each allMessage}}
    <label>From: {{this.sendBy}} | To: {{this.receivedBy}}</label>
    <label>Date: {{this.date}}</label>
    <label class="labelmessage-individual">{{this.message}}</label>
    
    <br />
  {{/each}}
</ul>
</div>
</div>
<div class="grid-container-commentinput">
    <div class="grid-item-commentinput">
<div
      action="/private/message"
      id="message-form"
      method="POST"
    >
      
<form
      action="/private/message/viewall/"
      id="message-form"
      method="POST"
    >
      <div class="form">
        <input type="text" name="sender" value={{{sender}}} readonly="true" hidden/>
        <input type="text" name="receiver" value={{{receiver}}} readonly="true" hidden/>

        <label>Reply</label>
        <input
          id="message"
          name="message"
          type="text"
          placeholder="Enter Message"
        />
        <br />
        <button type="submit" class="btn btn-primary">Send</button>
      </div>
    </form>
  
      </div>
      </div>

